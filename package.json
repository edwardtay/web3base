{
  "name": "verisense-agentkit",
  "version": "1.0.0",
  "description": "Cybersecurity agent for Web2 and Web3",
  "private": true,
  "author": "NetWatch Team",
  "license": "Apache-2.0",
  "scripts": {
    "start": "NODE_OPTIONS='--no-warnings' node dist/server.js",
    "start:prod": "NODE_OPTIONS='--no-warnings' node dist/server.js",
    "start:cli": "NODE_OPTIONS='--no-warnings' tsx ./src/index.ts",
    "server": "NODE_OPTIONS='--no-warnings --experimental-specifier-resolution=node' tsx ./src/server.ts",
    "dev": "nodemon --exec tsx ./src/index.ts",
    "dev:server": "nodemon --exec tsx ./src/server.ts",
    "build": "tsc",
    "vercel-build": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint -c .eslintrc.json src/**/*.ts",
    "lint:fix": "eslint -c .eslintrc.json src/**/*.ts --fix",
    "format": "prettier --write \"**/*.{ts,js,cjs,json,md}\"",
    "format:check": "prettier -c .prettierrc --check \"**/*.{ts,js,cjs,json,md}\""
  },
  "dependencies": {
    "@coinbase/agentkit": "^0.1.2",
    "@coinbase/agentkit-langchain": "latest",
    "@langchain/core": "^0.3.19",
    "@langchain/langgraph": "^0.2.21",
    "@langchain/openai": "^0.3.14",
    "@letta-ai/letta-client": "^1.3.1",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "moralis": "^2.27.2",
    "node-fetch": "^3.3.2",
    "viem": "^2.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vercel/node": "^5.5.13",
    "eslint": "^8.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  }
}
