{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/resolve-ens.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/resolve-ens",
      "dest": "/api/resolve-ens.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}

